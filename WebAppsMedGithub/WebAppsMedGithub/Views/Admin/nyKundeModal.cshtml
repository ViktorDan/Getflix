@model WebAppsMedGithub.Models.Kunde

<div id="nyKundeModal" class="modal fade" style="padding-top:10%;">
    <div class="modal-dialog" style="background-color:rgb(51, 51, 51); ">
        <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
            <h4 class="modal-title">Ny Kunde</h4>
        </div>
        <div class="modal-body">
            <p>Legg til kundens informasjon:</p>
            <div class="text" id="nyNavn">
                @{
                    Html.RenderPartial("kundeForm");
                }
            </div>
        </div>
        <div class="modal-footer">
            <button type="button" class="btn btn-default" data-dismiss="modal">Avbryt</button>
            <button type="button" class="btn btn-danger" data-dismiss="modal" id="nyModalKnapp">Legg til</button>
        </div>
    </div>
</div>
@Html.Partial("kundeForm")