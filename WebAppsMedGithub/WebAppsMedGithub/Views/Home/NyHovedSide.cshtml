@model List<System.String>
@{
    ViewBag.Title = "Kjøp film";
}
<script type="text/javascript">
    var url = window.location.href;
    var data = url.split('data=').pop();
    $(document).ready(function () {
        
        $('#data').load("/Home/HentFilmInfo", { Id: data});

    });
    function myAlert() {
        if (confirm('Bekreft Kjøp')) {
            $.load("Home/Bestilling", { Id: data });
        }

    }
    
</script>

<div id="navbar" class="navbar navbar-inverse navbar-fixed-top">
    <div class="container">
        <div class="navbar-header">
            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            @Html.ActionLink("Getflix", "Index", "Home", new { area = "" }, new { @class = "navbar-brand", id = "getflixKnapp" })
        </div>
        <div class="navbar-collapse collapse">
            <ul class="nav navbar-nav pull-right">
                <li>@Html.ActionLink("Logg ut", "LoggUt")</li>
            </ul>
        </div>
    </div>
</div>

<a id="tilbakeKnapp" href="/Home/HovedSide" class="previous round">&#8249;</a>

<br/>
<p id="data"></p>
<br/>

<div id="FilmListe">

</div>






