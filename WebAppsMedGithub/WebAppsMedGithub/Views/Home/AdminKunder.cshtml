@model List<WebAppsMedGithub.Models.dbKunder>
@{
    ViewBag.Title = "AdminKunder";
}

<div id="navbar" class="navbar navbar-inverse navbar-fixed-top">
    <div class="container">
        <div class="navbar-header">
            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            @Html.ActionLink("Getflix - Admin", "HovedSide", "Home", new { area = "" }, new { @class = "navbar-brand", id = "getflixKnapp" })
        </div>
        <div class="navbar-collapse collapse">
            <ul class="nav navbar-nav">
                <li>@Html.ActionLink("Kunder", "AdminKunder")</li>
                <li>@Html.ActionLink("Filmer", "AdminFilm")</li>
                <li>@Html.ActionLink("Bestilling", "AdminBestilling")</li>
            </ul>
            <ul class="nav navbar-nav pull-right">
                <li>@Html.ActionLink("Logg ut", "LoggUt")</li>
            </ul>
        </div>
    </div>
</div>

<h2>AdminKunder</h2>

<div>
    <table>
        <tr>
            <th>Brukernavn</th>
            <th>Fornavn</th>
            <th>Etternavn</th>
            <th>Adresse</th>
            <th>Postnummer</th>
            <th>Tlf</th>
        </tr>
        @foreach (var kunde in Model)
        {
            <tr>
                <td>@kunde.Brukernavn</td>
                <td>@kunde.Fornavn</td>
                <td>@kunde.Etternavn</td>
                <td>@kunde.Adresse</td>
                <td>@kunde.Postnr</td>
                <td>@kunde.Tlf</td>
            </tr>
        }
    </table>
</div>